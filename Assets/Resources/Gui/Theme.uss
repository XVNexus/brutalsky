.bs {
    -unity-font: resource('Fonts & Materials/Poppins-Regular');
    -unity-font-definition: resource('Fonts & Materials/Poppins-Regular SDF');
    font-size: 20px;
    color: rgb(255, 255, 255);
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    text-overflow: ellipsis;
    -unity-text-align: middle-center;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    transition-duration: 100ms;
    transition-timing-function: ease-out-sine;
}

.bs-container {
    justify-content: center;
    align-items: center;
    flex-grow: 1;
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
}

.bs-pane {
    background-color: rgba(26, 26, 26, 0.95);
    padding-top: 5px;
    padding-right: 5px;
    padding-bottom: 5px;
    padding-left: 5px;
    border-top-width: 5px;
    border-right-width: 5px;
    border-bottom-width: 5px;
    border-left-width: 5px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
    border-top-color: rgb(26, 26, 26);
    border-bottom-color: rgba(0, 0, 0, 0.3);
    border-right-color: rgba(0, 0, 0, 0.2);
    border-left-color: rgba(0, 0, 0, 0.2);
}

.bs-list > * > * > #unity-content-container {
    width: 100%;
    flex-direction: column;
    flex-wrap: nowrap;
}

.bs-grid > * > * > #unity-content-container {
    width: 100%;
    flex-direction: row;
    flex-wrap: wrap;
}

.bs-title {
    -unity-font-style: bold;
    font-size: 125%;
    color: rgb(255, 170, 0);
    background-color: rgb(26, 26, 26);
    margin-top: -10px;
    margin-right: -10px;
    margin-bottom: 6px;
    margin-left: -10px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
    border-top-width: 5px;
    border-right-width: 5px;
    border-bottom-width: 5px;
    border-left-width: 5px;
    border-bottom-color: rgb(18, 18, 18);
    border-top-color: rgb(33, 33, 33);
    height: 50px;
    border-right-color: rgb(31, 31, 31);
    border-left-color: rgb(31, 31, 31);
}

.bs-box {
    padding-top: 5px;
    padding-right: 5px;
    padding-bottom: 5px;
    padding-left: 5px;
}

.bs-row {
    flex-direction: row;
    width: 100%;
}

.bs-label {
    height: 40px;
    flex-grow: 1;
    flex-basis: 1%;
    margin-top: 1px;
    margin-right: 1px;
    margin-bottom: 1px;
    margin-left: 1px;
}

.bs-button {
    flex-grow: 1;
    flex-basis: 1%;
    margin-top: 1px;
    margin-right: 1px;
    margin-bottom: 1px;
    margin-left: 1px;
    background-color: rgba(255, 255, 255, 0.02);
    border-top-color: rgba(0, 0, 0, 0.2);
    border-bottom-color: rgba(255, 255, 255, 0.2);
    height: 40px;
}

.bs-button:focus {
    background-color: rgba(255, 255, 255, 0.04);
    font-size: 110%;
    border-bottom-width: 5px;
    border-top-width: 0;
}

.bs-button:hover {
    background-color: rgba(255, 255, 255, 0.04);
    font-size: 110%;
    border-bottom-width: 5px;
    border-top-width: 0;
}

.bs-button:active {
    background-color: rgba(255, 255, 255, 0.01);
    border-top-width: 5px;
    border-bottom-width: 0;
    font-size: 110%;
}

.bs-text {
    flex-grow: 1;
    flex-basis: 1%;
    margin-top: 1px;
    margin-right: 1px;
    margin-bottom: 1px;
    margin-left: 1px;
    background-color: rgba(255, 255, 255, 0.02);
    border-top-color: rgba(0, 0, 0, 0.2);
    border-bottom-color: rgba(255, 255, 255, 0.2);
    height: 40px;
}

.bs-text > Label {
    display: none;
}

.bs-text > #unity-text-input {
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    color: rgb(255, 255, 255);
    background-color: rgba(0, 0, 0, 0);
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
}

.bs-spacer {
    flex-grow: 1;
}

.bs-disabled {
    opacity: 0;
    display: none;
    scale: 0.5 0.5;
}

.unity-scroller {
    width: 30px;
    margin-top: 5px;
    margin-right: 5px;
    margin-bottom: 5px;
    margin-left: 5px;
}

.unity-scroller > #unity-low-button {
    width: 100%;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    background-color: rgba(255, 255, 255, 0.02);
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    transition-duration: 100ms;
    transition-timing-function: ease-out-sine;
}

.unity-scroller > #unity-low-button:hover {
    background-color: rgba(255, 255, 255, 0.04);
}

.unity-scroller > #unity-high-button {
    width: 100%;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    background-color: rgba(255, 255, 255, 0.02);
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    transition-duration: 100ms;
    transition-timing-function: ease-out-sine;
}

.unity-scroller > #unity-high-button:hover {
    background-color: rgba(255, 255, 255, 0.04);
}

.unity-scroller > #unity-slider {
    width: 100%;
}

.unity-scroller > * > * > * > #unity-tracker {
    background-color: rgba(255, 255, 255, 0.02);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}

.unity-scroller > * > * > * > #unity-dragger {
    position: absolute;
    left: 5px;
    width: 20px;
    background-color: rgba(255, 255, 255, 0.1);
    transition-duration: 100ms;
    transition-timing-function: ease-out-sine;
}

.unity-scroller > * > * > * > #unity-dragger:hover {
    background-color: rgba(255, 255, 255, 0.2);
}
